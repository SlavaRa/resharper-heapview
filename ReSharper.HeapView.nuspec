<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>$PackageId$</id>
    <version>1.2.3</version>
    <!-- update plugin ver in AssemblyInfo.cs plz -->
    <title>Heap Allocations Viewer</title>
    <authors>Kevin Gosse, from Alexander Shvedov's code</authors>
    <owners></owners>
    <projectUrl>https://github.com/kevingosse/resharper-heapview</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/kevingosse/resharper-heapview/master/Content/icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Highlights local object allocations, boxing, delegates and closure creations points.

      Original extension has been written by Alexander Shvedov. This is a re-packaged version for 2017.2.
    </description>
    <releaseNotes>
&#8226; ReSharper 2017.2 support
    </releaseNotes>
    <dependencies>
      <dependency id="$ReSharperDep$" version="$ReSharperVer$" />
    </dependencies>
    <tags>heap allocations performance boxing closure lambda</tags>
  </metadata>
  <files>
    <file src="bin\wave06\$Configuration$\ReSharper.HeapView.*" target="dotFiles\" />
  </files>
</package>
